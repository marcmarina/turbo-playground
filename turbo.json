{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": true,
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": true,
      "outputMode": "new-only"
    },
    "start": {
      "dependsOn": ["build"],
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "verify": {
      "dependsOn": ["build", "test"],
      "outputs": [],
      "cache": true
    }
  }
}
